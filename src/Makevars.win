PKG_CFLAGS = $(SHLIB_OPENMP_CFLAGS) -I./argon2
PKG_LIBS = $(SHLIB_OPENMP_CFLAGS)

ARGON2_OBJS = \
	argon2/blake2/blake2b.o \
	argon2/argon2.o \
	argon2/core.o \
	argon2/encoding.o \
	argon2/ref.o \
	argon2/thread.o

R_OBJS = \
	argon2_native.o \
	blake2.o \
	gen_nonce.o \
	pw_hash.o \
	raw_as_char.o

OBJECTS = $(ARGON2_OBJS) $(R_OBJS)
all: $(SHLIB)
$(SHLIB): $(OBJECTS) 

clean:
	rm -rf *.o *.so *.dll 
